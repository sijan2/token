{
  "expo": {
    "name": "Token Transit",
    "slug": "token-transit",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#009CD8"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.sijan.tokentransit"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#009CD8"
      },
      "package": "com.sijan.tokentransit"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "753476a8-21f1-4c46-9067-23724cc2fdb5"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/753476a8-21f1-4c46-9067-23724cc2fdb5"
    }
  }
}
